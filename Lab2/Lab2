var num = parseInt(prompt('Enter the number of terms: '));
let n1 = 0, n2 = 1, nextTerm;
if(num<3){
   alert("please eneter the valid number");
}
else{
    var arr=[];
    for (let i = 1; i <= number; i++) {
        arr.push(n1);
        nextTerm = n1 + n2
        n1 = n2;
        n2 = nextTerm;
    }
}
for(var i=0;i<number;i++){
    document.write(arr[i]);
}
}
